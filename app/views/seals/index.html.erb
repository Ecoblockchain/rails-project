<p id="notice"><%= notice %></p>

<h1>Listing Seals</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seals.each do |seal| %>
      <tr>
        <td><%= link_to 'Show', seal %></td>
        <td><%= link_to 'Edit', edit_seal_path(seal) %></td>
        <td><%= link_to 'Destroy', seal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seal', new_seal_path %>
